{
  "name": "mendix-react-redux-boilerplate",
  "version": "1.0.0",
  "description": "Starter kit for React and Redux Mendix Custom Widget boilerplate",
  "scripts": {
    "prestart": "babel-node tools/startMessage.js",
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "lint": "node_modules/.bin/esw webpack.* src/reactjs src/widget tools",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress tools/testSetup.js \"src/reactjs/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "create:mpk": "node ./tools/createMpk"
  },
  "author": "Gerhard Richard Edens",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^4.1.2",
    "jquery": "^3.0.0",
    "react": "15.0.2",
    "react-dom": "15.0.2",
    "react-redux": "4.4.5",
    "react-router": "2.4.0",
    "react-router-redux": "4.0.4",
    "redux": "3.5.2",
    "redux-thunk": "2.0.1",
    "toastr": "2.1.2"
  },
